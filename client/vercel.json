{
  "routes": [
    {
      "src": "/(.*).html",
      "dest": "/index.html"
    },
    {
      "src": "/(.*).json",
      "dest": "/index.html"
    },
    {
      "src": "/([^/]+)$",
      "dest": "/index.html"
    },
    {
      "src": "^/static/(.*)$",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "continue": true
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
